{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_course_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-course-progress.user-course-progress",
      "mappedBy": "user"
    },
    "lesson_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson-progress.lesson-progress",
      "mappedBy": "users_permissions_user"
    },
    "quiz_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz-progress.quiz-progress",
      "mappedBy": "users_permissions_user"
    },
    "unit_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::unit-progress.unit-progress",
      "mappedBy": "users_permissions_user"
    }
  }
}
