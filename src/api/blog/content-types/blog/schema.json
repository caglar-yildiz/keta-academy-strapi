{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "related_datas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    },
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "blogs"
    },
    "isPopular": {
      "type": "boolean",
      "default": false
    },
    "SEO": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Lise",
        "Üniversite",
        "Mezun",
        "Doktora",
        "Üniversite Hazırlık"
      ]
    }
  }
}
